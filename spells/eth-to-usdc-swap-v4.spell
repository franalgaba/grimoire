# Swap ETH for USDC on Uniswap V4 (native ETH â€” no wrapping needed)

spell EthToUsdcSwapV4

  version: "1.0.0"
  description: "Swap ETH for USDC on Uniswap V4"
  assets: [ETH, USDC]

  params:
    amount: 1000000000000000

  venues:
    uniswap_v4: @uniswap_v4

  on manual:
    uniswap_v4.swap(ETH, USDC, params.amount)
    emit swap_done(asset_in=ETH, asset_out=USDC, amount=params.amount)
