# Test: Built-in functions â€” min, max, abs

spell TestBuiltinFunctions

  version: "1.0.0"
  description: "Test built-in math functions in expressions"
  assets: []

  params:
    a: 150
    b: 300
    negative: -42

  on manual:
    smallest = min(params.a, params.b)
    largest = max(params.a, params.b)
    positive = abs(params.negative)
    diff = abs(params.a - params.b)
    emit math_results(min_val=smallest, max_val=largest, abs_neg=positive, abs_diff=diff)
