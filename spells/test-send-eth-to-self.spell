# Test: Send a tiny ETH amount to self (tests basic transfer action)

spell TestSendEthToSelf

  version: "1.0.0"
  description: "Send ETH to self â€” tests wallet transfer"
  assets: [ETH]

  params:
    amount: 100000000000000
    to: "0x0000000000000000000000000000000000000000"

  venues:
    wallet: @wallet

  on manual:
    wallet.transfer(ETH, params.amount, params.to)
    emit transfer_done(asset=ETH, amount=params.amount, to=params.to)
