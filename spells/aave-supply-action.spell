# Supply USDC to Aave V3

spell AaveSupplyAction

  version: "1.0.0"
  description: "Deposit USDC into Aave V3"

  assets: [USDC]

  params:
    amount: 500000000

  venues:
    aave_v3: @aave_v3

  on manual:
    aave_v3.deposit(USDC, params.amount)
    emit deposit_submitted(asset=USDC, amount=params.amount)
